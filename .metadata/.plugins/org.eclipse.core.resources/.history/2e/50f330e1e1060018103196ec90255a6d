package me.ankit.Spring.springcore.di.autowiring;

@Component
public class Engine {
	
	private String Size;
	
	
	public Engine(String Size) {
		
		this.Size = Size;
	}

	public String getSize() {
		return Size;
	}

	public void setSize(String size) {
		Size = size;
	}

	@Override
	public String toString() {
		return "Engine [Size=" + Size + "]";
	}
	
	
	

}
